{
  "extends": [
    "airbnb",
    "prettier"
  ],
  "plugins": [
    "prettier",
    "react-hooks",
    "import",
    "jsx-a11y",
    "react"
  ],
  "rules": {
    "react/prop-types": [
      1
    ],
    "react/jsx-curly-newline": 0,
    "react-hooks/rules-of-hooks": "error",
    "react-hooks/exhaustive-deps": "warn",
    "prettier/prettier": [
      "warn"
    ],
    "react/jsx-filename-extension": [
      0
    ],
    "no-underscore-dangle": [
      0
    ],
    "no-console": [
      0
    ],
    "argsIgnorePattern": "^_"
  }
}